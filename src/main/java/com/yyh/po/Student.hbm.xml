<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.yyh.po.Student" table="student" schema="springbootdemo">
        <id name="stuid">
            <column name="stuid" sql-type="int(11)"/>
        </id>
        <property name="stuname">
            <column name="stuname" sql-type="varchar(20)" length="20"/>
        </property>
        <property name="stupwd">
            <column name="stupwd" sql-type="varchar(30)" length="30"/>
        </property>
    </class>
</hibernate-mapping>